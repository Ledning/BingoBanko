                <Button x:Name="nResponse" Content="Cancel" Margin="0,58.38,3.979,33.958" d:LayoutOverrides="Height" HorizontalAlignment="Right" Width="75"/>
                <TextBlock x:Name="Text" Margin="39.71,12,38.979,0" TextWrapping="Wrap" VerticalAlignment="Top"/>
            </Grid>
        </GroupBox>
    </Grid>
</catel:UserControl>
