<Window x:Class="BankoProject.Views.PresentationScreenHostView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                   xmlns:metro="http://metro.mahapps.com/winfx/xaml/controls"
        mc:Ignorable="d"
        WindowStartupLocation="Manual" Height="{Binding WHeight}" Width="{Binding WWidth}" Focusable="False"
        Background="{x:Null}" Top="{Binding Top}" Left="{Binding Left}" ShowInTaskbar="False" Topmost="False" IsHitTestVisible="False" AllowsTransparency="True" WindowStyle="None" >
    <Grid  Background="{x:Null}" Height="{Binding WHeight}" Width="{Binding WWidth}" IsHitTestVisible="True">
        <Button Height="70" Width="150"  Content="lel"></Button>
    </Grid>
</Window>
