<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:cal="http://www.caliburnproject.org" x:Class="BankoProject.Views.PresentationScreen.NumberBarView"
             mc:Ignorable="d" 
             d:DesignHeight="1080" d:DesignWidth="1920">
  <Grid>
    <ItemsControl IsTabStop="False" ItemsSource="{Binding Event.BingoNumberQueue}">
      <ItemsControl.ItemTemplate>
        <DataTemplate>
          <Border Tag="{Binding}">
            <TextBox Text="{Binding Value, Mode=OneWayToSource}" Width="182" Background="Transparent" Margin="10,0,0,0" />
          </Border>
        </DataTemplate>
      </ItemsControl.ItemTemplate>
      <ItemsControl.ItemsPanel>
        <ItemsPanelTemplate>
          <StackPanel Height="180" Orientation="Horizontal" FlowDirection="RightToLeft" VerticalAlignment="Bottom">
          </StackPanel>
        </ItemsPanelTemplate>
      </ItemsControl.ItemsPanel>
    </ItemsControl>
  </Grid>
</UserControl>
