<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:cal="http://www.caliburnproject.org" x:Class="BankoProject.Views.PresentationScreen.NumberBarView"
             mc:Ignorable="d" 
             d:DesignHeight="1080" d:DesignWidth="1920">
  <Grid>
    <StackPanel Height="180" Orientation="Horizontal" FlowDirection="RightToLeft" VerticalAlignment="Bottom">
      <ItemsControl ItemsSource="{Binding Event.BingoNumberQueue}">
        <ItemsControl.Template>
          <ControlTemplate>
            <TextBox Text="{Binding Value}" Width="182" Background="Transparent" Margin="10,0,0,0" />
          </ControlTemplate>
        </ItemsControl.Template>
      </ItemsControl>
      
    </StackPanel>
  </Grid>
</UserControl>
